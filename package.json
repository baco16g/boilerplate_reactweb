{
  "name": "boilerplate_reactweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p del server build:*:dev open",
    "prod": "run-s del build:*:prod",
    "lint:js": "eslint './client/scripts/**/*.js'",
    "format:js": "eslint './client/scripts/**/*.js' --fix",
    "build:js:dev": "NODE_ENV=development webpack --config ./webpack/webpack.config.js --mode development --progress --color --watch",
    "build:js:prod": "NODE_ENV=production webpack --config ./webpack/webpack.config.js --mode production --progress --color",
    "build:html:prod": "pug ./src/templates --basedir ./src/templates/ --pretty --out ./dist/",
    "build:copy:dev": "cpx './static/**/{*.html,*.jpg,*.png,*.gif,*.mp4,*.mov,*.json,*.eot,*.svg,*.ttf,*.woff}' './dist' --watch",
    "build:copy:prod": "cpx './static/**/{*.html,*.jpg,*.png,*.gif,*.mp4,*.mov,*.json,*.eot,*.svg,*.ttf,*.woff}' './dist'",
    "server": "node ./server/index.js",
    "precommit": "lint-staged",
    "open": "opn http://localhost:3000",
    "del": "rimraf ./dist"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/scripts/**/*.js": [
      "yarn format:js",
      "yarn lint:js",
      "git add"
    ]
  },
  "author": "yuki.kobayashi",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "recompose": "^0.28.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^0.4.5",
    "typescript-fsa-redux-thunk": "^1.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/recompose": "^0.26.4",
    "@types/redux": "^3.6.0",
    "commitizen": "^2.10.1",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "prettier": "^1.14.2",
    "pug": "^2.0.3",
    "pug-cli": "https://github.com/pugjs/pug-cli.git",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "utility-types": "^2.0.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
