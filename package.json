{
  "name": "boilerplate_reactweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p del server build:*:dev open",
    "build": "run-s del build:*:prod",
    "lint:js": "eslint './client/scripts/**/*.js'",
    "format:js": "eslint './client/scripts/**/*.js' --fix",
    "build:js:dev": "NODE_ENV=development webpack --config ./webpack/webpack.config.js --watch",
    "build:js:prod": "NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "build:copy:dev": "cpx './static/**/{*.html,*.jpg,*.png,*.gif,*.mp4,*.mov,*.json,*.eot,*.svg,*.ttf,*.woff}' './dist' --watch",
    "build:copy:prod": "cpx './static/**/{*.html,*.jpg,*.png,*.gif,*.mp4,*.mov,*.json,*.eot,*.svg,*.ttf,*.woff}' './dist'",
    "server": "node ./server/index.js",
    "precommit": "lint-staged",
    "open": "opn http://localhost:3000",
    "del": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baco16g/boilerplate_reactweb.git"
  },
  "lint-staged": {
    "src/scripts/**/*.js": [
      "yarn format:js",
      "yarn lint:js",
      "git add"
    ]
  },
  "author": "yuki.kobayashi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/baco16g/boilerplate_reactweb/issues"
  },
  "homepage": "https://github.com/baco16g/boilerplate_reactweb#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "easy-peasy": "^1.7.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "universal-router": "^7.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/universal-router": "^7.0.0",
    "commitizen": "^3.0.5",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.3.0",
    "cz-customizable-ghooks": "^1.5.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.3",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "type-zoo": "^3.2.1",
    "typescript": "^3.2.2",
    "utility-types": "^3.2.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0"
  }
}
