{
  "compilerOptions": {
    "lib": ["esnext","dom"],
    "target": "es5",
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "react",
    "typeRoots": ["typings"],
    "baseUrl": "./src"
  },
}