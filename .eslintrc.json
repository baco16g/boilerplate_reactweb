{
  "plugins": [
    "react",
    "jest"
  ],
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "extends": [
    "teppeis/es2019",
    "teppeis/+typescript",
    "teppeis/+prettier",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  }
}